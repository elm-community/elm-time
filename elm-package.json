        "elm-lang/core": "5.0.0 <= v < 6.0.0",
        "elm-lang/lazy": "2.0.0 <= v < 3.0.0",
        "elm-tools/parser": "2.0.1 <= v < 3.0.0"
    },
    "elm-version": "0.18.0 <= v < 0.19.0"
